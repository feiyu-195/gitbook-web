{
    "title": "SkyFish",
    "description": "这是我的简介...",
    "language": "zh-hans",
    "styles": {
        
    },
    "scripts": {
        "start": "assets/umami.js"
    },
    "plugins": [
        "theme-comscore",
         "-lunr", 
         "-search", 
         "search-pro", 
         "expandable-chapters", 
         "chapter-fold", 
         "splitter", 
         "github",
         "page-toc-button",
         "back-to-top-button",
         "tbfed-pagefooter",
         "code",
         "accordion",
         "prism",
         "-sharing", 
         "sharing-plus",
         "hide-element"
    ], 
    "pluginsConfig": {
        "github": {
            "url": "https://github.com/feiyu-195"
        },
        "page-treeview": {
            "copyright": "Copyright &#169; aleen42",
            "minHeaderCount": "2",
            "minHeaderDeep": "2"
        },
        "page-toc-button": {
            "maxTocDepth": 2,
            "minTocSize": 2
        },
        "tbfed-pagefooter": {
            "copyright":"赣ICP备2023008387号-1 赣公网安备36052102100040号 Copyright ©2022 - 2024 By Feiyu",
            "modify_label": "最近一修改时间：",
            "modify_format": "YYYY-MM-DD HH:mm"
        },
        "code": {
            "copyButtons": true
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ]
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "sharing": {
            "douban": false,
            "facebook": true,
            "google": false,
            "hatenaBookmark": false,
            "instapaper": false,
            "line": false,
            "linkedin": false,
            "messenger": false,
            "pocket": false,
            "qq": false,
            "qzone": false,
            "stumbleupon": false,
            "twitter": true,
            "viber": false,
            "vk": false,
            "weibo": true,
            "whatsapp": false,
            "all": [
                "facebook", "google", "twitter",
                "weibo", "instapaper", "linkedin",
                "pocket", "stumbleupon"
            ]
        }
    }
}